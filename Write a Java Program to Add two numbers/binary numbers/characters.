public class AddBinary {
    public static void main(String[] args) {
        
        int n1 = 100;
        int n2 = 200;
        int b1= Integer.parseInt("11",2);
        int b2 = Integer.parseInt("11",2);
        int c1= 'A';
        int c2='B';

        int sumn = n1+n2;
        int sumb = b1+b2;
        int sumc = c1+c2;

        System.out.println("Addition of two numbers is "+sumn);
        System.out.println("Addition of two binary numbers is "+(Integer.toBinaryString(sumb)));
        System.out.println("Addition of two characters is "+sumc);
    }
}
